<% raise %>

<%= simple_form_for(@user, url: user_registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :role, as: :radio_buttons, collection: [['candidate' , 'Candidate'], ['company', 'Company']], label_method: :second, value_method: :first, input_html: { class: "" } %>
  </div>


  <div class="form-actions">
    <%= f.button :submit, "ok" %>
  </div>
<% end %>
